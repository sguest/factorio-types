{
  "name": "factorio-types",
  "version": "1.1.0",
  "description": "Typescript declarations for the Factorio mod API",
  "main": "index.d.ts",
  "repository": "https://github.com/sguest/factorio-types.git",
  "author": "Spencer Guest",
  "license": "MIT",
  "scripts": {
    "validate": "tsc -p tsconfig.json",
    "copy-source": "cpy src/*.d.ts dist",
    "build": "ts-node json_parser/parser.ts && yarn run copy-source",
    "rebuild": "ts-node json_parser/parser.ts -clean && yarn run copy-source"
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "cpy-cli": "^3.1.1",
    "node-html-parser": "^1.1.16",
    "ts-node": "^10.0.0",
    "typescript": "^5.4.2",
    "typescript-to-lua": "^1.25.1"
  },
  "files": [
    "index.d.ts",
    "tsconfig.base.json",
    "dist/**/*.d.ts"
  ],
  "factorioVersion": "1.1.107",
  "dependencies": {
    "lua-types": "^2.13.1"
  },
  "peerDependencies": {
    "typescript-to-lua": "^1.25.1"
  }
}